                     (tag "NO3-Immobilization")
                     (negate true)
                     (handle sum)
                     (dimension "kg N/ha/&"))
           (interval (tag "NO3-Content")
                     (path column "${column}" SoilNO3 M)
                     (spec fixed SoilNO3 M)
                     (dimension "kg N/ha"))
           (interval (tag "NO3-Tillage")
                     (path column "${column}" SoilNO3 tillage)
                     (spec fixed SoilNO3 tillage)
                     (handle sum)
                     (dimension "kg N/ha/&"))
           (interval (path column "${column}" residuals_N_soil)
                     (spec column default residuals_N_soil)
                     (handle sum)
                     (tag "Residuals_N")
                     (dimension "kg N/ha/&"))
           (interval (path column "${column}" OrganicMatter "*" tillage_N_soil)
                     (spec organic default tillage_N_soil)
                     (handle sum)
                     (tag "Tillage_Org_N")
                     (dimension "kg N/ha/&"))
           (interval (path column "${column}" OrganicMatter "*"
                           Bioincorporation N_added)
                     (spec fixed Bioincorporation N_added)
                     (tag "Bioincorporation")
                     (handle sum)
                     (dimension "kg N/ha/&"))
           (interval (tag "AOM")
                     (path column "${column}" OrganicMatter "*" am "*" om "*" N)
                     (spec fixed AOM N)
                     (dimension "kg N/ha"))
           (interval (tag "SOM")
                     (path column "${column}" OrganicMatter "*" som "*" N)
                     (spec fixed SOM N)
                     (dimension "kg N/ha"))
           (interval (tag "SMB")
                     (path column "${column}" OrganicMatter "*" smb "*" N)
                     (spec fixed SMB N)
                     (dimension "kg N/ha"))
           (interval (tag "Buffer")
                     (path column "${column}" OrganicMatter "*" buffer N)
                     (spec organic default buffer N)
                     (dimension "kg N/ha"))))
