(deflog "Soil nitrogen" column
  "Nitrogen input, output, transformation and content for the soil."
  (where "soil_nitrogen.dlf")
  (summary "Soil nitrogen balance" "Soil NH4 balance" "Soil NO3 balance"
           "Soil organic nitrogen balance")
  (when hourly)
  (entries (interval (path column "${column}" SoilNH4 S_external)
		     (spec fixed SoilNH4 S_external)
                     (tag "NH4-Incorp")
                     (handle sum)
                     (dimension "kg N/ha/&"))
           (flux_top (path column "${column}" SoilNH4 J)
		     (spec fixed SoilNH4 J)
                     (tag "NH4-In-Matrix")
                     (negate true)
                     (handle sum)
                     (dimension "kg N/ha/&"))
           (flux_top (path column "${column}" SoilNH4 J_p)
                     (spec fixed SoilNH4 J_p)
                     (tag "NH4-In-Macro")
                     (negate true)
                     (handle sum)
                     (dimension "kg N/ha/&"))
           (flux_bottom (path column "${column}" SoilNH4 J)
			(spec fixed SoilNH4 J)
                        (negate true)
                        (handle sum)
                        (tag "NH4-Leak-Matrix")
                        (dimension "kg N/ha/&"))
           (flux_bottom (path column "${column}" SoilNH4 J_p)
                        (spec fixed SoilNH4 J_p)
                        (negate true)
                        (handle sum)
                        (tag "NH4-Leak-Macro")
                        (dimension "kg N/ha/&"))
           (interval (path column "${column}" SoilNH4 S_drain)
		     (spec fixed SoilNH4 S_drain)
                     (tag "NH4-Drain")
                     (handle sum)
                     (dimension "kg N/ha/&"))
           (interval (path column "${column}" SoilNH4 S_root)
		     (spec fixed SoilNH4 S_root)
