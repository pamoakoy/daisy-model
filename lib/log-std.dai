                     (tag "NH4-Tillage")
                     (dimension "kg N/ha/&"))
           (flux_top (path column "${column}" SoilNO3 J)
		     (spec fixed SoilNO3 J)
                     (handle sum)
                     (negate true)
                     (tag "NO3-In-Matrix")
                     (dimension "kg N/ha/&"))
           (flux_top (path column "${column}" SoilNO3 J_p)
		     (spec fixed SoilNO3 J_p)
                     (negate true)
                     (handle sum)
                     (tag "NO3-In-Macro")
                     (dimension "kg N/ha/&"))
           (flux_bottom (path column "${column}" SoilNO3 J)
                        (spec fixed SoilNO3 J)
                        (tag "NO3-Leak-Matrix")
                        (handle sum)
                        (negate true)
                        (dimension "kg N/ha/&"))
           (flux_bottom (path column "${column}" SoilNO3 J_p)
                        (spec fixed SoilNO3 J_p)
                        (tag "NO3-Leak-Macro")
                        (handle sum)
                        (negate true)
