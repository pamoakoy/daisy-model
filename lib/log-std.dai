(defsummary "Field organic nitrogen balance" balance
  "Organic nitrogen balance for the complete system."
  (precision 3)
  (require_top true)
  (input "Residuals_N_top" "Residuals_N_root" "Org_N-Fertilizer"
         "NO3-Immobilization")
  (output "NH4-mineralization")
  (content "Org_N-Surface" "Org_N-Soil"))

(deflog "Field nitrogen" column
  "Nitrogen input, output, transformation and content for the system."
  (where "field_nitrogen.dlf")
  (summary "Field nitrogen balance" "Field NH4 balance" 
           "Field NO3 balance" "Field organic nitrogen balance"
           "Field crop nitrogen balance")
  (when hourly)
  (entries (number (path column "${column}" fertilized_NH4)
                   (spec column default fertilized_NH4)
                   (handle sum)
                   (tag "NH4-Fertilizer")
                   (dimension "kg N/ha/&"))
           (number (path column "${column}" volatilization)
                   (spec column default volatilization)
                   (handle sum)
                   (tag "NH4-Volatilization")
                   (dimension "kg N/ha/&"))
           (number (path weather "*" deposit NH4)
                   (spec fixed IM_soil_flux NH4)
                   (handle sum)
                   (tag "NH4-Deposit")
                   (dimension "kg N/ha/&"))
           (number (path column "${column}" Surface IM NH4)
                   (spec fixed IM_soil NH4)
                   (tag "NH4-Surface")
                   (dimension "kg N/ha"))
           (number (path column "${column}" Surface IM_runoff NH4)
                   (spec fixed IM_soil_flux NH4)
                   (handle sum)
                   (tag "NH4-Runoff")
                   (dimension "kg N/ha/&"))
           (flux_bottom (path column "${column}" SoilNH4 J)
                        (spec fixed SoilNH4 J)
                        (handle sum)
                        (negate true)
                        (tag "NH4-Leak-Matrix")
                        (dimension "kg N/ha/&"))
           (flux_bottom (path column "${column}" SoilNH4 J_p)
                        (spec fixed SoilNH4 J_p)
                        (handle sum)
                        (negate true)
                        (tag "NH4-Leak-Macro")
                        (dimension "kg N/ha/&"))
           (interval (path column "${column}" SoilNH4 S_drain)
                     (spec fixed SoilNH4 S_drain)
                     (tag "NH4-Drain")
                     (handle sum)
                     (negate true)
                     (dimension "kg N/ha/&"))
           (number (path column "${column}" Vegetation crops crops "*" Root NH4Upt)
                   (spec fixed RootSystem NH4Upt)
                   (tag "NH4-Crop")
                   (handle sum)
                   (dimension "kg N/ha/&"))
           (interval (path column "${column}" Nitrification NH4)
                     (spec column default Nitrification NH4)
                     (handle sum)
                     (tag "NH4-Nitrification")
                     (dimension "kg N/ha/&")) 
           (interval (path column "${column}" Nitrification NO3)
                     (spec column default Nitrification NO3)
                     (handle sum)
                     (tag "NO3-Nitrification")
                     (dimension "kg N/ha/&"))
           (interval (path column "${column}" Nitrification N2O)
                     (spec column default Nitrification N2O)
                     (handle sum)
                     (tag "N2O-Nitrification")
                     (dimension "kg N/ha/&"))
           (interval (path column "${column}" OrganicMatter "*" NH4_source)
                     (spec organic default NH4_source)
                     (tag "NH4-mineralization")
                     (handle sum)
                     (dimension "kg N/ha/&"))
           (interval (path column "${column}" SoilNH4 M)
                     (spec fixed SoilNH4 M)
                     (tag "NH4-Soil")
                     (dimension "kg N/ha"))
           (number (path column "${column}" fertilized_NO3)
                   (spec column default fertilized_NO3)
                   (handle sum)
                   (tag "NO3-Fertilizer")
                   (dimension "kg N/ha/&"))
           (number (path weather "*" deposit NO3)
                   (spec fixed IM_soil_flux NO3)
                   (handle sum)
                   (tag "NO3-Deposit")
                   (dimension "kg N/ha/&"))
           (number (path column "${column}" Surface IM NO3)
                   (spec fixed IM_soil NO3)
                   (tag "NO3-Surface")
                   (dimension "kg N/ha"))
           (number (path column "${column}" Surface IM_runoff NO3)
                   (spec fixed IM_soil_flux NO3)
                   (handle sum)
                   (tag "NO3-Runoff")
                   (dimension "kg N/ha/&"))
           (flux_bottom (path column "${column}" SoilNO3 J)
                        (spec fixed SoilNO3 J)
                        (tag "NO3-Leak-Matrix")
                        (handle sum)
                        (negate true)
                        (dimension "kg N/ha/&"))
           (flux_bottom (path column "${column}" SoilNO3 J_p)
                        (spec fixed SoilNO3 J_p)
                        (tag "NO3-Leak-Macro")
                        (handle sum)
                        (negate true)
                        (dimension "kg N/ha/&"))
           (interval (path column "${column}" SoilNO3 S_drain)
                     (spec fixed SoilNO3 S_drain)
                     (tag "NO3-Drain")
                     (negate true)
                     (handle sum)
                     (dimension "kg N/ha/&"))
           (number (path column "${column}" Vegetation crops crops "*" Root NO3Upt)
                   (spec fixed RootSystem NO3Upt)
                   (tag "NO3-Crop")
                   (handle sum)
                   (dimension "kg N/ha/&"))       ;g/m^2 -> kg N/ha
           (interval (path column "${column}" Denitrification converted)
                     (spec fixed Denitrification converted)
                     (tag "Denitrification")
                     (handle sum)
                     (dimension "kg N/ha/&"))
           (interval (path column "${column}" OrganicMatter "*" NO3_source)
                     (spec organic default NO3_source)
                     (tag "NO3-Immobilization")
                     (negate true)
                     (handle sum)
                     (dimension "kg N/ha/&"))
           (interval (tag "NO3-Soil")
                     (path column "${column}" SoilNO3 M)
                     (spec fixed SoilNO3 M)
                     (dimension "kg N/ha"))
           (number (path column "${column}" Vegetation crops crops "*" CrpN Fixated)
                   (spec fixed CrpN Fixated)
                   (handle sum)
                   (dimension "kg N/ha/&"))
           (number (path column "${column}" OrganicMatter "*" fertilized_N)
                   (spec organic default fertilized_N)
                   (handle sum)
                   (tag "Org_N-Fertilizer")
                   (dimension "kg N/ha/&"))
           (number (path column "${column}" seed_N)
                   (spec column default seed_N)
                   (handle sum)
                   (tag "Seed_N")
                   (dimension "kg N/ha/&"))
           (number (path column "${column}" harvest_N)
                   (spec column default harvest_N)
                   (handle sum)
                   (tag "Harvest_N")
                   (dimension "kg N/ha/&"))
           (number (path column "${column}" residuals_N_top)
                   (spec column default residuals_N_top)
                   (handle sum)
                   (tag "Residuals_N_top")
                   (dimension "kg N/ha/&"))
           (number (path column "${column}" residuals_N_root)
                   (spec column default residuals_N_root)
                   (handle sum)
                   (tag "Residuals_N_root")
                   (dimension "kg N/ha/&"))
           (number (tag "Org_N-Surface")
                   (path column "${column}" OrganicMatter "*" am "*" om "*" top_N)
                   (spec fixed AOM top_N)
                   (dimension "kg N/ha"))
           (interval (tag "Org_N-Soil")
                     (path column "${column}" OrganicMatter "*" total_N)
                     (spec organic default total_N)
                     (dimension "kg N/ha"))
           (number (tag "Crop")
                   (path column "${column}" Vegetation crops crops "*"
                         Prod NCrop)
                   (spec fixed Production NCrop)
                   (dimension "kg N/ha"))
           (number (tag "Dead leaves")
                   (path column "${column}" Vegetation crops crops "*"
                         Prod NDead)
                   (spec fixed Production NDead)
                   (dimension "kg N/ha"))))

