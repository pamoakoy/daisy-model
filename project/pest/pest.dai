  (summary (simple (print_sum false)
                   (precision 6)
                   (fetch ("W-Surface-Drain") ("W-Surface-Soil")
                          ("W-Biopore-0-5")("W-Biopore-5-30")
                          ("W-Biopore-30")("W-Matrix-30")))
