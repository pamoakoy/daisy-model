  ;; Watering left side.
  (wait (mm_dd  6 27))
  (irrigate_subsoil 2.133 [mm/h] (hours 3) (volume left)
                    (solute (NH4 0 [mg N/l])(NO3 0 [mg N/l])))
  (wait (mm_dd  6 28))
  (irrigate_subsoil 2 [mm/h] (hours 3) (volume left)
                    (solute (NH4 0 [mg N/l])(NO3 0 [mg N/l])))
  (wait (mm_dd  6 29))
  (irrigate_subsoil 2 [mm/h] (hours 3) (volume left)
                    (solute (NH4 0 [mg N/l])(NO3 0 [mg N/l])))
  (wait (mm_dd  6 30))
  (irrigate_subsoil 2 [mm/h] (hours 3) (volume left)
                    (solute (NH4 0 [mg N/l])(NO3 0 [mg N/l])))
  (wait (mm_dd  7  1))
  (irrigate_subsoil 2 [mm/h] (hours 3) (volume left)
                    (solute (NH4 0 [mg N/l])(NO3 0 [mg N/l])))
  (wait (mm_dd  7  3))
  (irrigate_subsoil 2 [mm/h] (hours 1) (volume left)
                    (solute (NH4 0 [mg N/l])(NO3 0 [mg N/l])))   
