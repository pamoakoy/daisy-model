  (irrigate_subsoil 2 [mm/h] (hours 1) (volume right)
                    (solute (NH4 0 [mg N/l])(NO3 0 [mg N/l])))
  (wait (mm_dd  6 21))
  (irrigate_subsoil 2 [mm/h] (hours 1) (volume right)
                    (solute (NH4 0 [mg N/l])(NO3 0 [mg N/l])))
  (wait (mm_dd  6 22))
