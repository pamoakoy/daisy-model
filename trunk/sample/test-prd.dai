  (irrigate_subsoil 2 [mm/h] (hours 1) (volume left)
                    (solute (NH4 0 [mg N/l])(NO3 0 [mg N/l])))        
  (wait (mm_dd  8  5))
  (irrigate_subsoil 2 [mm/h] (hours 1) (volume left)
                    (solute (NH4 0 [mg N/l])(NO3 0 [mg N/l])))
  (wait (mm_dd 8 15))
  (harvest "Potato; Exp" (stem 0.00)(leaf 0.00)(sorg 1.00)))

(manager "DK 2005 PRD")

;; Some additional log files.

(deflog "ABAExtraction" crop
  "ABA extraction in all soil intervals."
  (where "ABAExtraction.dlf")
  (when hourly)
  (entries ;;Year Month MDay Hour
           (array (tag "ABA")
		  (dimension "ng/cm^3/h")
		  (path column "${column}" 
			Vegetation crops crops "${crop}" Root ABAExtraction)
		  (spec fixed RootSystem ABAExtraction))))

(deflog "H2OExtraction" crop
  "H2O extraction in all soil intervals."
  (where "H2OExtraction.dlf")
  (when hourly)
  (entries ;;Year Month MDay Hour
           (array (tag "H2O")
		  (dimension "cm^3/cm^3/h")
		  (path column "${column}" 
			Vegetation crops crops "${crop}" Root H2OExtraction)
		  (spec fixed RootSystem H2OExtraction))))

(deflog "ABACrop" crop
  "ABA relevant information from crop."
  (where "ABACrop.dlf")
  (when hourly)
  (entries ;;Year Month MDay Hour
           (number (tag "ABAConc")
		   (dimension "ng/cm^3")
		   (path column "${column}" 
			 Vegetation crops crops "${crop}" Root ABAConc)
		   (spec fixed RootSystem ABAConc))
	   (number (tag "H2OUpt")
		   (dimension "mm/h")
		   (path column "${column}" 
			 Vegetation crops crops "${crop}" Root H2OUpt)
		   (spec fixed RootSystem H2OUpt)
		   (handle sum))
	   (number (tag "Stress")
		   (path column "${column}" Vegetation crops crops "${crop}"
                         Root water_stress)
		   (spec fixed RootSystem water_stress)
		   (handle max))))

(deflog "Root form GP2D" crop
  (where "rootform_gp2d.dlf")
  (when daily)
  (entries ;; Year Month MDay
           (number (path column "${column}" Vegetation crops crops "${crop}" 
                         Prod WRoot)
                   (spec fixed Production WRoot))
           (number (path column "${column}" Vegetation crops crops "${crop}" 
                         Phenology DS)
                   (spec phenology default DS))
           (number (path column "${column}" Vegetation crops crops "${crop}" 
                         Root Depth)
                   (spec fixed RootSystem Depth))
           (number (path column "${column}" Vegetation crops crops "${crop}" 
                         Root rootdens "*" a_z)
                   (spec rootdens GP2D a_z))
           (number (path column "${column}" Vegetation crops crops "${crop}" 
                         Root rootdens "*" a_x)
                   (spec rootdens GP2D a_x))
           (number (path column "${column}" Vegetation crops crops "${crop}" 
                         Root rootdens "*" L00)
                   (spec rootdens GP2D L00))
           (number (path column "${column}" Vegetation crops crops "${crop}" 
                         Root rootdens "*" k)
                   (spec rootdens GP2D k))
	   (interval (path column "${column}" Vegetation crops crops "${crop}" 
			   Root Density)
		     (spec fixed RootSystem Density)
		     (tag "0-1 m")
		     (from  0 [cm])
		     (to -100 [cm]))
	   (content (path column "${column}" Vegetation crops crops "${crop}" 
			  Root Density)
		    (spec fixed RootSystem Density)
		    (tag "0,0 cm")
		    (z 0 [cm])
		    (x 0 [cm]))
	   (content (path column "${column}" Vegetation crops crops "${crop}" 
			  Root Density)
		    (spec fixed RootSystem Density)
		    (tag "-10,0 cm")
		    (z -10 [cm])
		    (x 0 [cm]))
	   (content (path column "${column}" Vegetation crops crops "${crop}" 
			  Root Density)
		    (spec fixed RootSystem Density)
		    (tag "-50,0 cm")
		    (z -50 [cm])
		    (x 0 [cm]))
	   (content (path column "${column}" Vegetation crops crops "${crop}" 
			  Root Density)
		    (spec fixed RootSystem Density)
		    (tag "0,40 cm")
		    (z 0 [cm])
		    (x 10 [cm]))
	   (content (path column "${column}" Vegetation crops crops "${crop}" 
			  Root Density)
		    (spec fixed RootSystem Density)
		    (tag "-10,40 cm")
		    (z -10 [cm])
		    (x 10 [cm]))
	   (content (path column "${column}" Vegetation crops crops "${crop}" 
			  Root Density)
		    (spec fixed RootSystem Density)
		    (tag "-50,40 cm")
		    (z -50 [cm])
		    (x 10 [cm]))
	   (content (path column "${column}" Vegetation crops crops "${crop}" 
			  Root Density)
		    (spec fixed RootSystem Density)
		    (tag "0,50 cm")
		    (z 0 [cm])
		    (x 50 [cm]))
	   (content (path column "${column}" Vegetation crops crops "${crop}" 
			  Root Density)
		    (spec fixed RootSystem Density)
		    (tag "-10,50 cm")
		    (z -10 [cm])
		    (x 50 [cm]))
	   (content (path column "${column}" Vegetation crops crops "${crop}" 
			  Root Density)
		    (spec fixed RootSystem Density)
		    (tag "-50,50 cm")
		    (z -50 [cm])
		    (x 50 [cm]))
	   (content (path column "${column}" Vegetation crops crops "${crop}" 
			  Root Density)
		    (spec fixed RootSystem Density)
		    (tag "0,60 cm")
		    (z 0 [cm])
		    (x 10 [cm]))
	   (content (path column "${column}" Vegetation crops crops "${crop}" 
			  Root Density)
		    (spec fixed RootSystem Density)
		    (tag "-10,60 cm")
		    (z -10 [cm])
		    (x 10 [cm]))
	   (content (path column "${column}" Vegetation crops crops "${crop}" 
			  Root Density)
		    (spec fixed RootSystem Density)
		    (tag "-50,60 cm")
		    (z -50 [cm])
		    (x 10 [cm]))
	   (content (path column "${column}" Vegetation crops crops "${crop}" 
			  Root Density)
		    (spec fixed RootSystem Density)
		    (tag "-10,* cm")
		    (z -10 [cm]))
	   (content (path column "${column}" Vegetation crops crops "${crop}" 
			  Root Density)
		    (spec fixed RootSystem Density)
		    (tag "*,0 cm")
		    (x 0 [cm]))
	   (content (path column "${column}" Vegetation crops crops "${crop}" 
			  Root Density)
		    (spec fixed RootSystem Density)
		    (tag "0,* cm")
		    (z 0 [cm]))
	   (content (path column "${column}" Vegetation crops crops "${crop}" 
			  Root Density)
		    (spec fixed RootSystem Density)
		    (tag "*,40 cm")
		    (x 10 [cm]))
	   (content (path column "${column}" Vegetation crops crops "${crop}" 
			  Root Density)
		    (spec fixed RootSystem Density)
		    (tag "-50,* cm")
		    (z -50 [cm]))
	   (content (path column "${column}" Vegetation crops crops "${crop}" 
			  Root Density)
		    (spec fixed RootSystem Density)
		    (tag "*,50 cm")
		    (x 50 [cm]))))

;; Create these log files.
(output harvest
        (ABACrop (crop "Potato; Exp") 
		 (when (crop_ds_after "Potato; Exp" 0.0)))
	(ABAExtraction (crop "Potato; Exp")
		       (when (crop_ds_after "Potato; Exp" 0.0)))
	(H2OExtraction (crop "Potato; Exp") 
		       (when (crop_ds_after "Potato; Exp" 0.0)))
        ("Root form GP2D" (crop "Potato; Exp") 
                          (when (crop_ds_after "Potato; Exp" 0.0)))
	("Root Density" (crop "Potato; Exp") 
	                (when (crop_ds_after "Potato; Exp" 0.0)))
	("Soil Water Potential" 
         (when (crop_ds_after "Potato; Exp" 0.0)))
	("Field water")
        ("Crop Production" 
	 (crop "Potato; Folva; ABA")
         (where "sbarley.dlf")))

