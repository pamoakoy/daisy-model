     (Large_RtDR 0.05)
     (LfDR (0.00 0.00) (1.60 0.00) (1.75 0.02) (2.00 0.10))
     (RtDR (0.00 0.00) (1.60 0.00) (2.00 0.00))
     (ExfoliationFac 0.0))
 
  (CrpN (PtRootCnc (0.00 0.0200) (1.00 0.0200) (2.00 0.0100))
     (CrRootCnc (0.00 0.0100) (1.00 0.0100) (2.00 0.0080))
