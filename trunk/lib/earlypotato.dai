     (CrStemCnc (0.00 0.012) (1.25 0.012) (2.00 0.010))
     (NfStemCnc (0.00 0.004) (2.00 0.004))
     (PtSOrgCnc (1.00 0.020) (1.25 0.015) (2.00 0.012))
     (CrSOrgCnc (1.00 0.008) (1.25 0.008) (2.00 0.0075))
