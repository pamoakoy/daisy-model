      (MxNO3Up     2.5E-0007)
      (rootdens "Gerwitz+Page74" 
      (SpRtLength 200 [m/g])
      (DensRtTip 0.2 [cm/cm^3])))

  (Partit 
     (Root (0.00 0.20) (1.00 0.20) (1.36 0.00))
