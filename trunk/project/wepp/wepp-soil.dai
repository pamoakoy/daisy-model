(defcolumn Rorrende_led3 WEPP;; led 3
  "This is a Rorrende soil with pipe drains."
;;  (OrganicMatter none)
  ;;(Chemistry none)
    (Soil (MaxRootingDepth 150 [cm])
  (horizons (-3.00 "Overflade Ap_3")
            (-25.00 "Rorrende Ap_3")
            (-33.00 "Rorrende Bplow layer_3")
            (-120 "Rorrende Bt")
            (-200 "Rorrende C")))
  (Groundwater aquitard
        (K_aquitard 0.050 [cm/h])
        (Z_aquitard 200 [cm]))

  (SoilHeat (enable_ice true))
  (Movement original
  (Tertiary 
  (biopores (classes ("matrix_0cm") 
                     ("matrix_halfway")
                     ("matrix_2cm")
                     ("drain_2cm_S")
                     ("drain_2cm_W")
                     ("matrix_30cm")
                     ("drain_30cm"))))))
                     
(defcolumn Rorrende_fd3 Rorrende_led3
  "Rorrende soil with free drainiage."
  (Groundwater deep))         


(defcolumn Rorrende_led1 Rorrende_led3
  (Soil 
  (horizons (-3.00 "Overflade Ap_1")
            (-25.00 "Rorrende Ap_1")
            (-33.00 "Rorrende Bplow layer_1")
            (-120 "Rorrende Bt")
            (-200 "Rorrende C")))
  (Movement original
  (Tertiary 
  (biopores (classes ("matrix_0cm") 
                     ("matrix_2cm_R")
                     ("drain_2cm_S_R")
                     ("drain_2cm_W_R")
                     ("matrix_30cm")
                     ("drain_30cm"))))))          
 
 (defcolumn Rorrende_fd1 Rorrende_led1
  	"Rorrende soil with free drainiage."
	(Groundwater deep))
	
