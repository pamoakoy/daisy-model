(deflog "Soil Heat Balance" table
  (where "soil_heat_balance.dlf")
  (when hourly)
  (entries (year (path time))
           (month (path time))
           (mday (path time))
           (hour (path time))
           (number (path weather "*" hourly_air_temperature)
                   (tag "T_air")
                   (dimension "dg C"))
           (number (path column "$col" SoilHeat T_top)
                   (tag "T_top")
                   (dimension "dg C"))
           (index (path column "$col" SoilHeat T)
                  (tag "T_0")
                  (index 0)
                  (dimension "dg C"))
           (index (path column "$col" SoilHeat T)
                  (tag "T_1")
                  (index 1)
                  (dimension "dg C"))
           (index (path column "$col" SoilHeat T)
                  (tag "T_2")
                  (index 2)
                  (dimension "dg C"))
