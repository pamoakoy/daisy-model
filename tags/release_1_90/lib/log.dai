                  (tag "h_0")
                  (index 0)
                  (dimension "cm"))
           (index (path column "$col" SoilWater h)
                  (tag "h_1")
                  (index 1)
                  (dimension "cm"))
           (index (path column "$col" SoilWater h)
                  (tag "h_2")
                  (index 2)
                  (dimension "cm"))
           (index (path column "$col" SoilWater S_ice)
                  (tag "S_ice_0")
