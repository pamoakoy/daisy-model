                  (index 0)
                  (dimension "erg/cm^3/dg C"))
           (index (path column "$col" SoilHeat C_apparent)
                  (tag "C_apparent")
                  (index 0)
                  (dimension "erg/cm^3/dg C"))
           (index (path column "$col" SoilWater X_ice)
                  (tag "Ice_0")
                  (index 0)
