(deflog "Soil Nitrate Concentration" table
  (where "soil_nitrate_conc.dlf")
  (when daily)
  (entries (year (path time))
           (month (path time))
           (mday (path time))
           (array (path column "$col" SoilNO3 C)
                  (dimension "mg/l")
                  (factor 1.0e6))))     ;g/cm³ -> mg/l

(deflog "Soil Water NO3 Concentration" table
  (where "soil_NO3_conc.dlf")
  (when daily)
  (entries (year (path time))
           (month (path time))
           (mday (path time))
           (array (path column "$col" SoilNO3 C)
                  (dimension "mg/l")
                  (factor 4.43e6))))     ;g NO3-N/cm³ -> mg NO3/l

