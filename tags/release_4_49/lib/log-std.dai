           (interval (path column "${column}" Chemistry "*" 
                           trace "${chemical}" S_transform)
                     (tag "Soil Transform")
                     (handle sum)
                     (dimension "g/ha/&")
                     (spec chemical default S_transform))
           (number (path column "${column}" Chemistry "*" trace "${chemical}"
                         snow_storage)
                   (tag Snow)
